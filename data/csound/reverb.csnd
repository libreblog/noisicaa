<?xml version="1.0"?>
<csound>
<display-name>Reverb</display-name>
<ports>
  <port name="in:left" type="audio" direction="input"/>
  <port name="in:right" type="audio" direction="input"/>
  <port name="out:left" type="audio" direction="output">
    <drywet port="in:left" default="0"/>
    <bypass port="in:left"/>
  </port>
  <port name="out:right" type="audio" direction="output">
    <drywet port="in:right" default="0"/>
    <bypass port="in:right"/>
  </port>
  <port name="mix" type="kratecontrol" direction="input">
    <float-control min="0" max="1" default="1"/>
    <display-name>Mix</display-name>
  </port>
  <port name="feedback" type="kratecontrol" direction="input">
    <float-control min="0" max="1" default="0.7"/>
    <display-name>Feedback level</display-name>
  </port>
  <port name="cutoff" type="kratecontrol" direction="input">
    <float-control min="0" max="20000" default="2000"/>
    <display-name>Cutoff frequency</display-name>
  </port>
</ports>
<orchestra>
0dbfs = 1.0
ksmps = 32
nchnls = 2

gaInL      chnexport "in:left", 1
gaInR      chnexport "in:right", 1

gaOutL     chnexport "out:left", 2
gaOutR     chnexport "out:right", 2

gkMix      chnexport "mix", 1
gkFeedback chnexport "feedback", 1
gkCutoff   chnexport "cutoff", 1

instr 1
    gaOutL, gaOutR reverbsc gaInL, gaInR, gkFeedback, gkCutoff
    aOutL = gaOutL + gkMix * gaInL
    gaOutR = gaOutR + gkMix * gaInR
endin
</orchestra>
<score>
i1 0 -1
</score>
</csound>
